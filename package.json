{
  "name": "boilup",
  "description": "Boilerplate code generator",
  "version": "0.0.0-alpha.03",
  "author": "Jakub Hajduk",
  "keywords": [
    "boilerplate",
    "code",
    "generator",
    "init",
    "starter",
    "programmatic",
    "esm",
    "typed",
    "files",
    "write"
  ],
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "pkgroll --watch",
    "build": "pkgroll",
    "check": "biome check",
    "fix": "biome check --fix"
  },
  "engines": {
    "node": ">=20.0"
  },
  "dependencies": {
    "boilup": "link:",
    "writepool": "^0.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^20.14.5",
    "changelogen": "^0.5.7",
    "pkgroll": "^2.1.1",
    "tsx": "^4.15.6",
    "typescript": "^5.4.5"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  }
}
